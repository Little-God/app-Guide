<!doctype html>
<html>

	<head>
		<meta charset="UTF-8">
		<title></title>
		<meta name="viewport" content="width=device-width,initial-scale=1,minimum-scale=1,maximum-scale=1,user-scalable=no" />
		<link href="../css/mui.min.css" rel="stylesheet" />
		<link rel="stylesheet" type="text/css" href="../css/iconfont.css"/>
		<link rel="stylesheet" type="text/css" href="../css/style.css"/>
		<style type="text/css">
			.mui-flex1{padding: 10px 0px;margin: 0 30px;}
			.ret{border-bottom: 2px solid red;color: red;}
		</style>
	</head>

	<body>
		<!--头部-->
		<header class="mui-bar mui-bar-nav">
		   	<span class="mui-action-back mui-icon mui-icon-left-nav mui-pull-left orange"></span><span class="vipitem orange mui-action-back backsize">返回</span>
		    <h1 class="mui-title bold">退货管理</h1>
		</header>
		<!--内容-->
		<div class="mui-content">
			<!--处理状态按钮-->
		    <div class="mui-flex statebox">
		    	<div class="mui-flex1 ret">
		    		处理中
		    	</div>
		    	<div class="mui-flex1">
		    		已处理
		    	</div>
		    </div>
		    <!--无订单box-->
		    <div class="noorder">
		    	<div class="">
		    		<span class="icon-noorder iconfont icon-dingdan"></span>
		    	</div>
		    	没有订单记录
		    </div>
		</div>
		<script src="../js/mui.min.js"></script>
		<script src="../js/jquery_min.js" type="text/javascript" charset="utf-8"></script>
		<script src="../js/app.js" type="text/javascript" charset="utf-8"></script>
		<script type="text/javascript">
			mui.init();
			$('.statebox .mui-flex1').click(function(){
				$(this).addClass('ret').siblings().removeClass('ret');
			});
		</script>
	</body>

</html>